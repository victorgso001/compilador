<Token> ::= <int-lit> | <float-lit> | <bool-lit> | <id> | <op-ad> |
            <op-mul> | <op-rel> | := | "(" | ")" | "[" | "]" | . |
            .. | , | ; | : | eof

<int-lit> ::= <digito> | <int-lit><digito>

<float-lit> ::= <int-lit> . <int-lit> | <int-lit> . | . <int-lit>

<bool-lit> ::= true | false

<id> ::= <letra> | <id> <letra> | <id> <digito>

<op-ad> ::= + | - | or

<op-mul> ::= * | / | and

<op-rel> ::= < | > | <= | >= | = | <>

<separator> ::= space | eol
